{%  block table %}
        <table class="table">
            <tr>
                <th>Nom</td>
                <th>Mail</td>
                <th>Message</td>
                <th>Publié</td>
                <th>Vu</td>
            </tr>
        {% for result in results %}
            <tr>
                <td>{{result.name}}</td>
                <td>{{result.mail}}</td>
                <td>{{result.msg | raw}}</td>
                <td>
                {% if (result.pub == 1) %}
                <a class="btn btn-success" onclick="bj(this)" data-bj="admin|adminContactPub|publish=0,id={{result.id}}">Publié</a>
                {% else %}
                <a class="btn btn-warning" onclick="bj(this)" data-bj="admin|adminContactPub|publish=1,id={{result.id}}">Dépublié</a>
                {% endif %}
                </td>
                <td>
                    <a class="btn btn-link" href="/displayContact/{{result.id}}" onclick="return bh(this)">Voir</a>
                </td>
            </tr>
        {% endfor %}
        </table>
{%  endblock table %}
